module App

use UnityEngine

public class Test: MonoBehaviour {
    public var value = 0.0
    private var body: Rigidbody

    public fn start() {
        body = getComponent<Rigidbody>()
        body.addForce(force: Vector3(x: 0.0f, y: 0.0f, z: value), mode: ForceMode::force)
    }

    public fn update() {
        value += 1 * Time::deltaTime
    }
}

public fn freeFunc() {

}