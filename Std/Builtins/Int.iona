module Builtins

public struct Int {
    private var value: nint

    public init(value: Int) {
        self.value = value
    }

    public init() {
        self.value = 0
    }

    public op +(left: Builtins.Int, right: Int) -> Int {
        var newInt = Int()
        newInt.value = left.value + right.value

        return newInt
    }

    public op -(left: Int, right: Int) -> Int {
        var newInt = Int()
        newInt.value = left.value - right.value

        return newInt
    }
}